@inject IPasswordService PasswordService
@inject NavigationManager Nav

@page "/"

@code {
	protected override async Task OnInitializedAsync()
	{
		bool isSet = await PasswordService.HasKey();
		if (isSet)
		{
			Nav.NavigateTo(SiteRoutes.Login, true);
		}
		else
		{
			Nav.NavigateTo(SiteRoutes.Register, true);
		}
	}
}